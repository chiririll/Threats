<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Style Selector="TextBlock.h1">
    <Setter Property="FontSize" Value="16"/>
    <Setter Property="FontWeight" Value="SemiBold"/>
    <Setter Property="Foreground" Value="#0039a6"/>
    <Setter Property="TextWrapping" Value="Wrap"/>
    <Setter Property="VerticalAlignment" Value="Center"/>
    <Setter Property="Margin" Value="0,0,0,15"/>
  </Style>

  <Style Selector="TextBlock.blue-colored">
    <Setter Property="Foreground" Value="#0039a6"/>
  </Style>

  <Style Selector="Bold.h1sel">
    <Setter Property="Foreground" Value="#6F00FF"/>
  </Style>

  <Style Selector="Window">
    <Setter Property="Background" Value="#F6F0F0"/>
  </Style>

  <Style Selector="Rectangle.bg">
    <Setter Property="Fill" Value="#F6F0F0"/>
  </Style>

  <Style Selector="Rectangle.yesnoeven">
    <Setter Property="Fill" Value="#F2E2B1"/>
  </Style>

  <Style Selector="Rectangle.yesnoodd">
    <Setter Property="Fill" Value="#F6F0F0"/>
  </Style>
</Styles>
